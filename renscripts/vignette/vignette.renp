props vignette {
  enabled: toggle {
    default: true,
    description: "Enable vignette effect"
  }
  
  weight: range {
    default: 0.4,
    min: 0.0,
    max: 1.0,
    description: "Vignette intensity"
  }
  
  stretch: range {
    default: 0.15,
    min: 0.0,
    max: 1.0,
    description: "Vignette stretch amount"
  }
  
  cameraFov: range {
    default: 0.8,
    min: 0.1,
    max: 3.0,
    description: "Camera FOV for vignette calculation"
  }
}