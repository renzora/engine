script Vignette {

  start {
    log("ðŸ“· Initializing vignette effect...")
    
    // Enable vignette with initial settings
    enableVignette(enabled)
    setVignetteWeight(weight)
    setVignetteStretch(stretch)
    setVignetteCameraFov(cameraFov)
    
    // Get current vignette settings to verify
    vignetteSettings = getVignetteSettings()
    
    log("âœ… Vignette effect initialized")
