    log("  Camera FOV: " + vignetteSettings.cameraFov)
  }

  update() {
    // Update vignette settings based on properties
    enableVignette(enabled)
    setVignetteWeight(weight)
    setVignetteStretch(stretch)
    setVignetteCameraFov(cameraFov)
  }

  destroy {
