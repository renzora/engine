{
  "productName": "vite-solid-starter",
  "version": "0.0.0",
  "identifier": "com.tauri.vite-solid-starter",
  "build": {
    "beforeBuildCommand": "npm run build",
    "beforeDevCommand": "npm run dev",
    "frontendDist": "../dist/client",
    "devUrl": "http://localhost:5173"
  },
  "app": {
    "windows": [
      {
        "title": "vite-solid-starter",
        "width": 800,
        "height": 600,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png"
    ]
  }
}